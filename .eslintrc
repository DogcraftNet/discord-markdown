{
    "env": {
		"browser": true,
		"amd": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module",
		"requireConfigFile": false,
		"parser": "@typescript-eslint/parser"
	},
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
		"prettier",
		"@typescript-eslint"
	],
    "rules": {
        "prettier/prettier": [
			"error"
		],
		"@typescript-eslint/no-explicit-any": 0
    }
}
