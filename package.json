{
	"name": "@odiffey/discord-markdown",
	"version": "3.1.0",
	"description": "A markdown parser for Discord messages",
	"keywords": [
		"discord",
		"markdown",
		"parser",
		"md"
	],
	"files": [
		"dist"
	],
	"main": "./dist/discord-markdown.umd.js",
	"module": "./dist/discord-markdown.mjs",
	"types": "./dist/src/main.d.ts",
	"repository": "https://github.com/odiffey/discord-markdown.git",
	"author": "Owen Diffey <owen@diffey.dev>",
	"contributors": [
		"brussell98",
		"Sorunome"
	],
	"license": "MIT",
	"private": false,
	"scripts": {
		"build": "vite build && tsc --emitDeclarationOnly",
		"lint": "eslint src tests --ext .ts",
		"test": "TZ=UTC vitest"
	},
	"exports": {
		".": {
			"import": "./dist/discord-markdown.mjs",
			"require": "./dist/discord-markdown.umd.js"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"dependencies": {
		"highlight.js": "^11.9.0",
		"@khanacademy/simple-markdown": "^0.11.3"
	},
	"devDependencies": {
		"@types/node": "^20.11.20",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "3.2.5",
		"terser": "^5.28.1",
		"typescript": "^5.3.3",
		"vite": "^5.1.4",
		"vitest": "^1.3.1"
	}
}
